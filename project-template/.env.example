# Supabase Configuration
# Copy this to .env and fill in your values

# Get these from: npx supabase status
VITE_SUPABASE_URL=https://xxx.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# For CLI operations (generate at: https://supabase.com/dashboard/account/tokens)
SUPABASE_ACCESS_TOKEN=sbp_your_token_here

# Your project reference (from URL: https://xxx.supabase.co)
SUPABASE_PROJECT_REF=your_project_ref_here

# For direct database access (optional)
# Get from Supabase Dashboard → Settings → Database → Connection String (Transaction mode)
SUPABASE_DB_URL=postgresql://postgres:password@db.xxx.supabase.co:5432/postgres
